@article{RefWorks:doc:58929128e4b0228a292928a7,
	author={J. E. Stehr and S. L. Chen and S. Filippov and M. Devika and N. Koteeswara Reddy and C. W. Tu and W. M. Chen and I. A. Buyanova},
	year={2013},
	month={Jan 11,},
	title={Defect properties of ZnO nanowires revealed from an optically detected magnetic resonance study},
	journal={Nanotechnology},
	volume={24},
	number={1},
	pages={015701},
	abstract={Optically detected magnetic resonance (ODMR) complemented by photoluminescence measurements is used to evaluate optical and defect properties of ZnO nanowires (NWs) grown by rapid thermal chemical vapor deposition. By monitoring visible emissions, several grown-in defects are revealed and attributed to Zn vacancies, shallow (but not effective mass) donor and exchange-coupled pairs of Zn vacancies and Zn interstitials. It is also found that the intensity of the donor-related ODMR signals is substantially lower in the NWs compared with that in bulk ZnO. This may indicate that formation of native donors is suppressed in NWs, which is beneficial for achieving p-type conductivity.},
	isbn={0957-4484},
	language={English},
	url={http://www.ncbi.nlm.nih.gov/pubmed/23221124},
	pmid={23221124}
}
